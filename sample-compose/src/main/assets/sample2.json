{
  "components": {
    "StoryItem": {
      "state": {
        "image": "",
        "text": ""
      },
      "rootComponent": {
        "componentType": "Box",
        "children": [
          {
            "componentType": "Box",
            "layout_padding": {
              "start": 4,
              "end": 4,
              "top": 8,
              "bottom": 8
            },
            "children": [
              {
                "componentType": "Image",
                "src": "{{image}}",
                "contentScale": "Crop",
                "layout_width": "72",
                "layout_height": "92"
              },
              {
                "componentType": "Text",
                "text": "{{text}}",
                "textSize": 12,
                "layout_width": "fillMax",
                "layout_alignment": "BottomStart",
                "layout_padding": {
                  "start": 8,
                  "end": 8,
                  "top": 8,
                  "bottom": 8
                }
              }
            ]
          }
        ]
      }
    },
    "ContactItem2": {
      "state": {
        "avatar": "",
        "name": "",
        "lastSeen": "",
        "online": false,
        "targetLastSeen": {
          "functionType": "Condition",
          "value": "{{online}}",
          "trueResult": "Online",
          "falseResult": "{{lastSeen}}"
        },
        "onClick": null
      },
      "rootComponent": {
        "componentType": "Box",
        "onClick": "{{onClick}}",
        "children": [
          {
            "componentType": "Row",
            "layout_width": "fillMax",
            "layout_padding": {
              "start": 16,
              "end": 16,
              "top": 8,
              "bottom": 8
            },
            "children": [
              {
                "componentType": "Box",
                "layout_width": 72,
                "layout_height": 72,
                "children": [
                  {
                    "componentType": "Image",
                    "src": "{{avatar}}",
                    "contentScale": "Crop",
                    "clip": "Circle",
                    "layout_width": "fillMax",
                    "layout_height": "fillMax"
                  },
                  {
                    "functionType": "Condition",
                    "value": "{{online}}",
                    "trueResult": {
                      "componentType": "Image",
                      "src": "https://zibuhoker.ru/ifm/indicator.png",
                      "layout_alignment": "BottomEnd",
                      "contentScale": "Inside",
                      "clip": "Circle",
                      "layout_width": 24,
                      "layout_height": 24,
                      "layout_padding": {
                        "end": 2,
                        "bottom": 2
                      }
                    },
                    "falseResult": null
                  }
                ]
              },
              {
                "componentType": "Column",
                "layout_width": "fillMax",
                "children": [
                  {
                    "componentType": "Text",
                    "text": "{{name}}",
                    "textSize": 16,
                    "layout_width": "fillMax",
                    "layout_padding": {
                      "top": 8,
                      "start": 8
                    }
                  },
                  {
                    "componentType": "Text",
                    "text": "{{targetLastSeen}}",
                    "textSize": 12,
                    "layout_width": "fillMax",
                    "layout_padding": {
                      "top": 8,
                      "start": 8,
                      "bottom": 8
                    }
                  }
                ]
              }
            ]
          }
        ]
      }
    },
    "StoriesContainerItem": {
      "state": {
        "stories": []
      },
      "rootComponent": {
        "componentType": "LazyRow",
        "children": "{{stories}}"
      }
    },
    "ContactScreen2": {
      "state": {
        "title": "",
        "contacts": []
      },
      "rootComponent": {
        "componentType": "Column",
        "children": [
          {
            "componentType": "Column",
            "children": {
              "functionType": "CombineArrays",
              "arrays": [
                {
                  "componentType": "Toolbar",
                  "title": "{{title}}"
                },
                {
                  "componentType": "LazyColumn",
                  "layout_height": "fillMax",
                  "children": "{{contacts}}"
                }
              ]
            }
          }
        ]
      }
    }
  },
