{
  "components": {},
  "state": {
    "id": "rootShort2",
    "componentType": "LazyColumn",
    "children": [
      {
        "componentType": "Button",
        "text": "Next JSON",
        "onClick": {
          "interactionType": "ValueCallback",
          "value": "sample1.json"
        }
      },
      {
        "id": "input",
        "componentType": "Input",
        "onTextChanged": {
          "interactionType": "StatePatch",
          "patch": {
            "id": "input",
            "text": "{{args.text}}"
          }
        },
        "text": {
          "fieldType": "Primitive",
          "id": "inputText",
          "value": "Default"
        }
      }
    ]
  }
}